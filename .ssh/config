IgnoreUnknown UseKeyChain
UseKeychain yes
Compression yes
GSSAPIAuthentication yes
GSSAPIDelegateCredentials yes

Host ns1.adelman.com ns2.adelman.com ns3.adelman.com ns4.adelman.com
  VerifyHostKeyDNS yes
  ProxyJump servfail.adelman.com

Host servfail.adelman.com www.yoyodyne.com kdc.eludom.net gatekeeper.electric-loft.org
  VerifyHostKeyDNS yes
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes
  ForwardAgent yes

# Local network machines
Host om om.eludom.net
  HostName om.eludom.net
  User ddp
  ForwardX11 yes
  ForwardX11Trusted yes

Host fluffy fluffy.eludom.net
  HostName fluffy.eludom.net
  User ddp
  ForwardX11 yes
  ForwardX11Trusted yes

Host lambda lambda.local lambda.eludom.net
  HostName lambda.local
  User ddp
  ForwardX11 yes
  ForwardX11Trusted yes
  StrictHostKeyChecking accept-new

# Local network wildcard - accept new host keys
Host *.eludom.net *.electric-loft.org 192.168.0.*
  StrictHostKeyChecking accept-new
  ForwardX11 yes
  ForwardX11Trusted yes

Host *
  ControlMaster auto
  ControlPath ~/.ssh/cm-%r@%h:%p
  ControlPersist 10m
