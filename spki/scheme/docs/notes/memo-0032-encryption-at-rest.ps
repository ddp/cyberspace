(Encrypted CAS) show newline
(-------------) show newline
() show newline
() show newline
(    \(define \(cas-put-encrypted data\)) show newline
(      "Store encrypted object in CAS") show newline
(      \(let \(\(hash \(content-hash data\)\)            ; Hash plaintext) show newline
(             \(key \(derive-content-key \(vault-key\) hash\)\)) show newline
(             \(encrypted \(encrypt-symmetric data key\)\)\)) show newline
(        \(secure-clear! key\)) show newline
(        \(storage-put hash encrypted\)               ; Store ciphertext) show newline
(        \(soup-put hash encrypted: #t\)) show newline
(        hash\)\)) show newline
(    \(define \(cas-get-decrypted hash\)) show newline
(      "Retrieve and decrypt object from CAS") show newline
(      \(let \(\(encrypted \(storage-get hash\)\)) show newline
(             \(key \(derive-content-key \(vault-key\) hash\)\)) show newline
(             \(data \(decrypt-symmetric encrypted key\)\)\)) show newline
(        \(secure-clear! key\)) show newline
(        ;; Verify hash) show newline
(        \(unless \(equal? hash \(content-hash data\)\)) show newline
(          \(error "Hash verification failed after decryption"\)\)) show newline
(        data\)\)) show newline
() show newline
() show newline
(Encrypted Soup) show newline
(--------------) show newline
() show newline
() show newline
(    \(define \(soup-put-encrypted hash metadata\)) show newline
(      "Store encrypted soup entry") show newline
(      \(let \(\(serialized \(serialize metadata\)\)) show newline
(             \(key \(derive-key \(vault-key\) 'metadata\)\)) show newline
(             \(encrypted \(encrypt-symmetric serialized key\)\)\)) show newline
(        \(secure-clear! key\)) show newline
(        \(soup-storage-put hash encrypted\)\)\)) show newline
(    \(define \(soup-get-decrypted hash\)) show newline
(      "Retrieve and decrypt soup entry") show newline
(      \(let \(\(encrypted \(soup-storage-get hash\)\)) show newline
(             \(key \(derive-key \(vault-key\) 'metadata\)\)) show newline
(             \(serialized \(decrypt-symmetric encrypted key\)\)\)) show newline
(        \(secure-clear! key\)) show newline
(        \(deserialize serialized\)\)\)) show newline
() show newline
() show newline
(Encrypted Indexes) show newline
(-----------------) show newline
() show newline
() show newline
(    ;; Searchable encryption for indexes) show newline
(    \(define \(index-put-encrypted term hash\)) show newline
(      "Add encrypted index entry") show newline
(      \(let* \(\(key \(derive-key \(vault-key\) 'index\)\)) show newline
(             \(encrypted-term \(deterministic-encrypt term key\)\)) show newline
(             \(encrypted-hash \(encrypt-symmetric hash key\)\)\)) show newline
(        \(secure-clear! key\)) show newline
(        \(index-storage-put encrypted-term encrypted-hash\)\)\)) show newline
(    ;; Deterministic encryption enables equality search) show newline
(    \(define \(deterministic-encrypt data key\)) show newline
(      "Deterministic encryption for searchable indexes") show newline
(      \(let \(\(derived-key \(hkdf-sha256 key salt: data length: 32\)\)\)) show newline
(        \(siv-encrypt data derived-key\)\)\)) show newline
