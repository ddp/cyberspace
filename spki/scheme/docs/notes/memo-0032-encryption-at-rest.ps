(------------------------------------------------------------------------) show newline
() show newline
() show newline
(Key Types) show newline
(---------) show newline
() show newline
() show newline
(    \(define key-hierarchy) show newline
(      '\(master-key                    ; Root of trust) show newline
(        +-- vault-key                 ; Per-vault encryption) show newline
(            +-- content-key           ; Object encryption) show newline
(            +-- metadata-key          ; Soup encryption) show newline
(            +-- index-key             ; Index encryption) show newline
(            +-- audit-key\)\)           ; Audit log encryption) show newline
(    \(define \(derive-key parent purpose\)) show newline
(      "Derive child key from parent") show newline
(      \(hkdf-sha256 parent) show newline
(                   salt: \(purpose->salt purpose\)) show newline
(                   info: \(string->utf8 \(symbol->string purpose\)\)) show newline
(                   length: 32\)\)) show newline
() show newline
() show newline
(Master Key) show newline
(----------) show newline
() show newline
() show newline
(    ;; Master key from key ceremony \(Memo-022\)) show newline
(    \(define \(initialize-master-key shares threshold\)) show newline
(      "Reconstruct master key from shares") show newline
(      \(let \(\(master \(shamir-combine shares threshold\)\)\)) show newline
(        ;; Derive vault key) show newline
(        \(let \(\(vault-key \(derive-key master 'vault\)\)\)) show newline
(          ;; Store encrypted vault key) show newline
(          \(store-encrypted-vault-key vault-key master\)) show newline
(          ;; Clear master from memory) show newline
(          \(secure-clear! master\)) show newline
(          vault-key\)\)\)) show newline
() show newline
() show newline
(Key Derivation) show newline
(--------------) show newline
() show newline
() show newline
(    \(define \(derive-content-key vault-key object-hash\)) show newline
(      "Derive unique key for each object") show newline
(      \(hkdf-sha256 vault-key) show newline
(                   salt: \(string->utf8 object-hash\)) show newline
(                   info: #u8\(99 111 110 116 101 110 116\)  ; "content") show newline
(                   length: 32\)\)) show newline
(    \(define \(derive-chunk-key content-key chunk-index\)) show newline
(      "Derive key for each chunk within object") show newline
(      \(hkdf-sha256 content-key) show newline
(                   salt: \(integer->bytevector chunk-index\)) show newline
(                   info: #u8\(99 104 117 110 107\)  ; "chunk") show newline
(                   length: 32\)\)) show newline
() show newline
() show newline
