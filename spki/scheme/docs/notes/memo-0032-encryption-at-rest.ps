(Key Storage) show newline
(-----------) show newline
() show newline
() show newline
(    ;; Keys encrypted with master key) show newline
(    \(define \(store-vault-key vault-key master-key\)) show newline
(      "Store vault key encrypted with master key") show newline
(      \(let \(\(encrypted \(encrypt-symmetric vault-key master-key\)\)\)) show newline
(        \(write-file \(vault-key-path\) encrypted\)\)\)) show newline
(    ;; Keys can also be stored as SPKI-encrypted blobs) show newline
(    \(define \(store-key-for-principal key principal\)) show newline
(      "Store key encrypted for specific principal") show newline
(      \(let* \(\(encrypted \(age-encrypt key \(list \(principal-public-key principal\)\)\)\)) show newline
(             \(hash \(cas-put encrypted\)\)\)) show newline
(        \(soup-put hash) show newline
(          type: 'encrypted-key) show newline
(          recipient: principal\)) show newline
(        hash\)\)) show newline
() show newline
() show newline
(Key Rotation) show newline
(------------) show newline
() show newline
() show newline
(    \(define \(rotate-vault-key\)) show newline
(      "Rotate vault encryption key") show newline
(      \(let \(\(old-key \(vault-key\)\)) show newline
(            \(new-key \(generate-key 32\)\)\)) show newline
(        \(audit-append action: 'key-rotation-start\)) show newline
(        ;; Re-encrypt all objects) show newline
(        \(for-each \(lambda \(hash\)) show newline
(                    \(let* \(\(encrypted \(storage-get hash\)\)) show newline
(                           \(data \(decrypt-symmetric encrypted old-key\)\)) show newline
(                           \(new-encrypted \(encrypt-symmetric data new-key\)\)\)) show newline
(                      \(storage-put hash new-encrypted\)\)\)) show newline
(                  \(all-object-hashes\)\)) show newline
(        ;; Update vault key) show newline
(        \(set-vault-key! new-key\)) show newline
(        \(secure-clear! old-key\)) show newline
(        \(audit-append action: 'key-rotation-complete\)\)\)) show newline
() show newline
() show newline
(Key Escrow) show newline
(----------) show newline
() show newline
() show newline
(    ;; Threshold key escrow for recovery) show newline
(    \(define \(escrow-vault-key vault-key custodians threshold\)) show newline
(      "Split vault key among custodians") show newline
(      \(let \(\(shares \(shamir-split vault-key \(length custodians\) threshold\)\)\)) show newline
(        \(for-each \(lambda \(custodian share\)) show newline
(                    \(let \(\(encrypted-share) show newline
(                           \(age-encrypt share \(list \(custodian-public-key custodian\)\)\)\)\)) show newline
(                      \(send-to-custodian custodian encrypted-share\)\)\)) show newline
(                  custodians shares\)\)\)) show newline
(    \(define \(recover-vault-key custodian-shares\)) show newline
(      "Recover vault key from custodian shares") show newline
(      \(let \(\(decrypted-shares) show newline
(             \(map \(lambda \(share custodian\)) show newline
(                    \(age-decrypt share \(custodian-identity custodian\)\)\)) show newline
(                  custodian-shares custodians\)\)\)) show newline
(        \(shamir-combine decrypted-shares\)\)\)) show newline
