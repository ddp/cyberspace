(Age Format) show newline
(----------) show newline
() show newline
() show newline
(    ;; Use age for object encryption \(Memo-018\)) show newline
(    \(define \(encrypt-object data recipient-keys\)) show newline
(      "Encrypt object using age format") show newline
(      \(age-encrypt data recipient-keys\)\)) show newline
(    \(define \(decrypt-object encrypted identity\)) show newline
(      "Decrypt object using age format") show newline
(      \(age-decrypt encrypted identity\)\)) show newline
() show newline
() show newline
(Symmetric Encryption) show newline
(--------------------) show newline
() show newline
() show newline
(    ;; For internal encryption with derived keys) show newline
(    \(define \(encrypt-symmetric data key\)) show newline
(      "Encrypt with ChaCha20-Poly1305") show newline
(      \(let \(\(nonce \(generate-nonce 12\)\)\)) show newline
(        \(let \(\(ciphertext \(chacha20-poly1305-encrypt data key nonce\)\)\)) show newline
(          \(bytevector-append nonce ciphertext\)\)\)\)) show newline
(    \(define \(decrypt-symmetric encrypted key\)) show newline
(      "Decrypt ChaCha20-Poly1305") show newline
(      \(let \(\(nonce \(subbytevector encrypted 0 12\)\)) show newline
(            \(ciphertext \(subbytevector encrypted 12\)\)\)) show newline
(        \(chacha20-poly1305-decrypt ciphertext key nonce\)\)\)) show newline
() show newline
() show newline
(Streaming Encryption) show newline
(--------------------) show newline
() show newline
() show newline
(    \(define \(encrypt-stream input-port output-port key\)) show newline
(      "Stream encryption for large objects") show newline
(      \(let \(\(chunk-size 65536\)) show newline
(            \(chunk-index 0\)\)) show newline
(        \(let loop \(\)) show newline
(          \(let \(\(chunk \(read-bytevector chunk-size input-port\)\)\)) show newline
(            \(unless \(eof-object? chunk\)) show newline
(              \(let \(\(chunk-key \(derive-chunk-key key chunk-index\)\)\)) show newline
(                \(write-bytevector \(encrypt-symmetric chunk chunk-key\) output-port\)) show newline
(                \(secure-clear! chunk-key\)\)) show newline
(              \(set! chunk-index \(+ chunk-index 1\)\)) show newline
(              \(loop\)\)\)\)\)\)) show newline
() show newline
() show newline
