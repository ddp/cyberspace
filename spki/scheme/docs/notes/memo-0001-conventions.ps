/Courier findfont 10 scalefont setfont
/margin 72 def
/pagewidth 612 def
/pageheight 792 def
/leading 12 def
/topmargin pageheight margin sub def
/bottommargin margin def
/linewidth pagewidth margin 2 mul sub def
/ypos topmargin def
/pagenum 1 def

/phosphor { 0.2 1 0.2 setrgbcolor } def
/darkbg { 0 0 0 setrgbcolor } def
/violet { 0.518 0.369 0.761 setrgbcolor } def
/gold { 1 0.843 0 setrgbcolor } def
/teal { 0 0.831 0.667 setrgbcolor } def
