/orange { 1 0.4 0 setrgbcolor } def
/coral { 1 0.2 0.4 setrgbcolor } def
/cyan { 0 1 1 setrgbcolor } def
/fillpage {
  gsave darkbg
  0 0 pagewidth pageheight rectfill
  grestore phosphor
} def

0.5 setlinewidth
1 setlinecap
1 setlinejoin
/newline {
  /ypos ypos leading sub def
  ypos bottommargin lt {
    showpage
    fillpage
    /pagenum pagenum 1 add def
    /ypos topmargin def
  } if
  margin ypos moveto
} def

