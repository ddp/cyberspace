(Result Type) show newline
(-----------) show newline
() show newline
() show newline
(    ;; Operations return \(ok value\) or \(err error\)) show newline
(    \(define \(result-ok value\) \(ok ,value\)\)) show newline
(    \(define \(result-err error\) \(err ,error\)\)) show newline
(    \(define \(result-ok? r\) \(eq? \(car r\) 'ok\)\)) show newline
(    \(define \(result-err? r\) \(eq? \(car r\) 'err\)\)) show newline
(    \(define \(result-value r\) \(cadr r\)\)) show newline
(    \(define \(result-error r\) \(cadr r\)\)) show newline
(    ;; Chain operations) show newline
(    \(define \(result-bind r f\)) show newline
(      \(if \(result-ok? r\)) show newline
(          \(f \(result-value r\)\)) show newline
(          r\)\)) show newline
(    ;; Example) show newline
(    \(define \(safe-read hash\)) show newline
(      \(result-bind \(cas-get-safe hash\)) show newline
(        \(lambda \(data\)) show newline
(          \(result-bind \(verify-signature data\)) show newline
(            \(lambda \(verified\)) show newline
(              \(result-ok verified\)\)\)\)\)\)) show newline
() show newline
() show newline
(Error Recovery) show newline
(--------------) show newline
() show newline
() show newline
(    \(define \(with-recovery operation recovery\)) show newline
(      "Execute operation with recovery on failure") show newline
(      \(let \(\(result \(guard \(ex \(else \(result-err ex\)\)\)) show newline
(                      \(result-ok \(operation\)\)\)\)\)) show newline
(        \(if \(result-ok? result\)) show newline
(            \(result-value result\)) show newline
(            \(recovery \(result-error result\)\)\)\)\)) show newline
(    ;; Example: read with fallback to replica) show newline
(    \(define \(read-with-fallback hash\)) show newline
(      \(with-recovery) show newline
(        \(lambda \(\) \(cas-get hash\)\)) show newline
(        \(lambda \(err\)) show newline
(          \(audit-append action: 'read-fallback error: err\)) show newline
(          \(cas-get-from-replica hash\)\)\)\)) show newline
() show newline
() show newline
(Retry Logic) show newline
(-----------) show newline
() show newline
() show newline
(    \(define \(with-retry operation max-retries backoff\)) show newline
(      "Retry operation with exponential backoff") show newline
(      \(let loop \(\(attempt 1\)\)) show newline
(        \(let \(\(result \(guard \(ex \(else \(result-err ex\)\)\)) show newline
(                        \(result-ok \(operation\)\)\)\)\)) show newline
(          \(cond) show newline
(            \(\(result-ok? result\)) show newline
(             \(result-value result\)\)) show newline
(            \(\(>= attempt max-retries\)) show newline
(             \(error "Max retries exceeded" \(result-error result\)\)\)) show newline
(            \(else) show newline
(             \(let \(\(delay \( backoff \(expt 2 \(- attempt 1\)\)\)\)\)) show newline
(               \(audit-append action: 'retry attempt: attempt delay: delay\)) show newline
(               \(thread-sleep! delay\)) show newline
(               \(loop \(+ attempt 1\)\)\)\)\)\)\)\)) show newline
(    ;; Retry configuration) show newline
(    \(define default-retry-config*) show newline
(      `\(\(max-retries . 3\)) show newline
(        \(initial-backoff . 100\)    ; ms) show newline
(        \(max-backoff . 30000\)      ; ms) show newline
(        \(jitter . 0.1\)\)\)           ; 10% randomization) show newline
() show newline
() show newline
(Circuit Breaker) show newline
(---------------) show newline
() show newline
() show newline
(    \(define \(make-circuit-breaker threshold timeout\)) show newline
(      \(let \(\(failures 0\)) show newline
(            \(state 'closed\)) show newline
(            \(last-failure #f\)\)) show newline
(        \(lambda \(operation\)) show newline
(          \(case state) show newline
(            \(\(open\)) show newline
(             \(if \(> \(- \(current-time\) last-failure\) timeout\)) show newline
(                 \(begin) show newline
(                   \(set! state 'half-open\)) show newline
(                   \(try-operation operation\)\)) show newline
(                 \(error "Circuit open"\)\)\)) show newline
(            \(\(half-open\)) show newline
(             \(let \(\(result \(try-operation operation\)\)\)) show newline
(               \(if \(result-ok? result\)) show newline
(                   \(begin) show newline
(                     \(set! state 'closed\)) show newline
(                     \(set! failures 0\)) show newline
(                     \(result-value result\)\)) show newline
(                   \(begin) show newline
(                     \(set! state 'open\)) show newline
(                     \(error "Circuit reopened"\)\)\)\)\)) show newline
(            \(\(closed\)) show newline
(             \(try-operation operation\)\)\)\)\)\)) show newline
(    \(define \(try-operation cb operation\)) show newline
(      \(guard \(ex) show newline
(              \(else) show newline
(               \(cb-record-failure! cb\)) show newline
(               \(result-err ex\)\)\)) show newline
(        \(result-ok \(operation\)\)\)\)) show newline
