(Manual Recovery) show newline
(---------------) show newline
() show newline
() show newline
(    \(define \(queue-manual-recovery error\)) show newline
(      "Queue error for manual intervention") show newline
(      \(soup-put) show newline
(        `\(recovery-ticket) show newline
(          \(error ,\(error-id error\)\)) show newline
(          \(status pending\)) show newline
(          \(created ,\(current-time\)\)) show newline
(          \(assigned #f\)\)) show newline
(        type: 'recovery-ticket\)\)) show newline
(    \(define \(process-recovery-ticket ticket-hash\)) show newline
(      "Process manual recovery ticket") show newline
(      \(let \(\(ticket \(soup-get ticket-hash\)\)\)) show newline
(        ;; Mark in progress) show newline
(        \(soup-update ticket-hash status: 'in-progress\)) show newline
(        ;; ... manual recovery steps ...) show newline
(        ;; Mark resolved) show newline
(        \(soup-update ticket-hash) show newline
(                     status: 'resolved) show newline
(                     resolved-at: \(current-time\)) show newline
(                     resolved-by: \(current-principal\)\)\)\)) show newline
