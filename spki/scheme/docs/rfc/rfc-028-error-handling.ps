(Conflict Detection) show newline
(------------------) show newline
() show newline
() show newline
(    \(define \(detect-conflict hash versions\)) show newline
(      "Detect conflicting versions of object") show newline
(      \(let \(\(unique-contents \(delete-duplicates) show newline
(                              \(map version-content versions\)\)\)\)) show newline
(        \(when \(> \(length unique-contents\) 1\)) show newline
(          \(record-error!) show newline
(            \(make-error 'conflict) show newline
(              "Conflicting versions detected") show newline
(              `\(\(hash . ,hash\)) show newline
(                \(versions . ,\(length unique-contents\)\)) show newline
(                \(sources . ,\(map version-source versions\)\)\)\)\)\)\)\)) show newline
() show newline
() show newline
(Conflict Resolution) show newline
(-------------------) show newline
() show newline
() show newline
(    \(define \(resolve-conflict hash versions strategy\)) show newline
(      "Resolve conflicting versions") show newline
(      \(let \(\(resolved) show newline
(             \(case strategy) show newline
(               \(\(latest\)) show newline
(                \(max-by version-timestamp versions\)\)) show newline
(               \(\(merge\)) show newline
(                \(merge-versions versions\)\)) show newline
(               \(\(manual\)) show newline
(                \(queue-for-manual-resolution hash versions\)\)) show newline
(               \(\(tombstone\)) show newline
(                \(create-conflict-tombstone hash versions\)\)\)\)\)) show newline
(        \(audit-append action: 'conflict-resolved) show newline
(                      hash: hash) show newline
(                      strategy: strategy\)) show newline
(        resolved\)\)) show newline
() show newline
