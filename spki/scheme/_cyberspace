#compdef cyberspace

# Zsh completion for Cyberspace Scheme REPL
# Install: cp _cyberspace /usr/local/share/zsh/site-functions/
# Or add to fpath: fpath=(~/.zsh/completions $fpath)

_cyberspace() {
  local -a commands
  commands=(
    'help:show help'
    'soup:browse vault objects'
    'status:show system status'
    'keys:list keys'
    'enroll-request:request enrollment'
    'enroll-listen:listen for enrollment requests'
    'enrollment-status:show enrollment status'
    'inspect:inspect an object'
    'bt:backtrace'
    'introspect-system:system info'
  )

  _describe 'cyberspace commands' commands
}

_cyberspace "$@"
